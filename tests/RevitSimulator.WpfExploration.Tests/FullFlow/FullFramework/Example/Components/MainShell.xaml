<Window x:Class="RevitSimulator.WpfExploration.Tests.FullFlow.FullFramework.Example.Components.MainShell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:RevitSimulator.WpfExploration.Tests.Navigation.Naive.Components"
        mc:Ignorable="d"
        Title="MainShell" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <StackPanel>
                <Button x:Name="ComponentAButton" Content="Component A"  Command="{Binding ComponentACommand}"/>
                <Button x:Name="ComponentBButton" Content="Component B"  Command="{Binding ComponentBCommand}"/>
                <Button x:Name="NextButton" Content="Next"  Command="{Binding NextCommand}"/>
                <Button x:Name="PreviousButton" Content="Previous"  Command="{Binding PreviousCommand}"/>
            </StackPanel>
            <ContentControl x:Name="ContentRegion"/>
        </StackPanel>
    </Grid>
</Window>
